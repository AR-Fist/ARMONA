                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                    <TextView
                        android:id="@+id/ArrPitch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toStartOf="@+id/ArrRoll"
                        app:layout_constraintStart_toEndOf="@+id/ArrYaw"
                        app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ArrRoll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>
        </ScrollView>

