                <TextView
                    android:id="@+id/MOPitch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toStartOf="@+id/MORoll"
                    app:layout_constraintStart_toEndOf="@+id/MOYaw"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/MORoll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphKalman"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="Kalmanfilter"
                    tools:ignore="HardcodedText" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                    <TextView
                        android:id="@+id/KalYaw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                    <TextView
                        android:id="@+id/KalPitch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toStartOf="@+id/KalRoll"
                        app:layout_constraintStart_toEndOf="@+id/KalYaw"
                        app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/KalRoll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphArrow"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="Arrow rotation"
                    tools:ignore="HardcodedText" />

