<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="arViewModel"
            type="com.arfist.armona.screen.ar.ArViewModel" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".screen.ar.ArFragment">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphRotVec"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="RotationVector"
                    tools:ignore="HardcodedText" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/RotVecYaw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/RotVecPitch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toStartOf="@+id/RotVecRoll"
                        app:layout_constraintStart_toEndOf="@+id/RotVecYaw"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/RotVecRoll"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphGGOrient"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="Google orientation"
                    tools:ignore="HardcodedText" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <TextView
                        android:id="@+id/GGOYaw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/GGOPitch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toStartOf="@+id/GGORoll"
                        app:layout_constraintStart_toEndOf="@+id/GGOYaw"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/GGORoll"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/testPlaceholder"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/text_size_test"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphComplementary"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="ComplementaryFilter"
                    tools:ignore="HardcodedText" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/ComYaw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ComPitch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toStartOf="@+id/ComRoll"
                    app:layout_constraintStart_toEndOf="@+id/ComYaw"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ComRoll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/testPlaceholder"
                    android:textColor="?android:textColorPrimary"
                    android:textSize="@dimen/text_size_test"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

                <com.jjoe64.graphview.GraphView
                    android:id="@+id/graphMyOrient"
                    android:layout_width="match_parent"
                    android:layout_height="240dp"
                    android:title="ComplementaryFilter"
                    tools:ignore="HardcodedText" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/MOYaw"
